{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "AppChannelsTester",
    "author": "Overwolf",
    "version": "1.0.11",
    "minimum-overwolf-version": "0.102.213.0",
    "description": "Test App Channels Functionality",
    "icon": "IconMouseNormal.png",
    "icon_gray": "IconMouseOver.png",
    "store_icon": "IconMouseNormal.png",
    "dock_button_title": "AppChannelsTester"
  },
  "permissions": [
    "VideoCaptureSettings",
    "Hotkeys",
    "Logging",
    "GameInfo",
    "FileSystem",
    "Extensions",
    "Profile",
    "Media",
    "Streaming",
    "DesktopStreaming"
  ],
  "hide_from_store": true,
  "data": {
    "start_window": "index",
    "windows": {
      "index": {
        "file": "index.html",
        "native_window": true,
        "resizable": true,
        "transparent": false,
        "show_in_taskbar": true,
        "use_os_windowing": true,
        "popup_blocker": true,
        "allow_local_file_access": true,
        "size": {
          "width": 400,
          "height": 230
        },
        "min_size": {
          "width": 400,
          "height": 230
        },
        "max_size": {
          "width": 400,
          "height": 230
        },
        "start_position": {
          "Top": 100,
          "Left": 300
        }
      }
    },
    "force_browser": "user",
    "hotkeys": {
      "show_app": {
        "title": "Show App",
        "action-type": "toggle",
        "default": "ctrl+z",
        "passthrough": false,
        "hold": false
      }
    },
    "game_targeting": {
      "type": "all"
    }
  }
}
