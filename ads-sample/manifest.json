{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "Ads Sample App",
    "version": "1.0.4",
    "minimum-overwolf-version": "0.165.0",
    "author": "Overwolf",
    "icon": "icons/IconMouseOver.png",
    "icon_gray": "icons/IconMouseNormal.png",
    "launcher_icon": "icons/launcher.ico",
    "window_icon": "icons/IconMouseOver.png",
    "description": ""
  },
  "data": {
    "start_window": "background",
    "protocol_override_domains": {
      "googlesyndication": "http"
    },
    "game_targeting": {
      "type": "all"
    },
    "windows": {
      "background": {
        "file": "background.html",
        "is_background_page": true,
        "background_optimization": false
      },
      "desktop": {
        "file": "desktop.html",
        "transparent": true,
        "native_window": true,
        "resizable": true,
        "show_in_taskbar": true,
        "desktop_only": true,
        "show_maximize": true,
        "keep_window_location": true,
        "override_on_update": true,
        "block_top_window_navigation": true,
        "popup_blocker": true,
        "mute": true,
        "size": {
          "width": 1212,
          "height": 656
        },
        "min_size": {
          "width": 1212,
          "height": 656
        },
        "start_position": {
          "top": 300,
          "left": 300
        }
      },
      "ingame": {
        "file": "ingame.html",
        "transparent": true,
        "resizable": true,
        "show_in_taskbar": false,
        "restrict_to_game_bounds": true,
        "in_game_only": true,
        "keep_window_location": true,
        "override_on_update": true,
        "block_top_window_navigation": true,
        "popup_blocker": true,
        "mute": true,
        "size": {
          "width": 1212,
          "height": 656
        },
        "min_size": {
          "width": 1212,
          "height": 656
        },
        "start_position": {
          "top": 300,
          "left": 300
        }
      }
    }
  }
}
